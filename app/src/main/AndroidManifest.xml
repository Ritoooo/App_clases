<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ucv.android.principal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContentFrmPrincipalEstudiante"></activity>

        <service
            android:name="ucv.android.controlador.EstudianteControlador"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="ucv.android.controlador.PersonalControlador"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".ContentFrmPrincipalPersonal" />
        <activity
            android:name=".FrmLoginEstudiante"
            android:label="@string/login_estudiante"/>
        <activity
            android:name=".FrmLoginPersonal"
            android:label="@string/login_personal"/>
        <activity android:name=".index">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>